@{
}

@model IEnumerable<Wikipages.Models.Business>

<html>
<head>
    <title>Wikipagest</title>
</head>
<body>
    <h2>Businesses List</h2>
    <h4>@Html.DisplayNameFor(model => model.Name)</h4>
    <ul>
        @foreach (var listBusiness in Model)
        {
            <li>@Html.DisplayFor(modelBusiness => listBusiness.Name) | | @Html.ActionLink("Details", "Details", new { id = listBusiness.BusinessId })</li>
        }
    </ul>
</body>
</html>